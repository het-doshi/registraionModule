{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/.*\\.css",
      "headers": { "Content-Type": "text/css" },
      "continue": true
    },
    { "src": "/.*", "dest": "/index.js" }
  ],
   "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "NODE_ENV": "production" }
    },
    {
      "src": "package.json",
      "use": "@now/npm"
    }
  ]
}
